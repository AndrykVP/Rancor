{
   "name": "andrykvp/rancor",
   "description": "A Laravel Package to make use of common features required by factions or groups of Star Wars Combine",
   "version": "2.0.0",
   "type": "library",
   "keywords": [
      "laravel",
      "star wars",
      "star wars combine",
      "swcombine",
      "swc"
   ],
   "license": "MIT",
   "authors": [
      {
         "name": "Andrés Velázquez",
         "email": "andriclucid@gmail.com",
         "role": "Developer"
      }
   ],
   "require": {
      "php": "^7.0|^8.0",
      "laravel/framework": "^8.0",
      "mews/purifier": "^3.2",
      "guzzlehttp/guzzle": "^7.0"
   },
   "require-dev": {
      "orchestra/testbench": "^6.0",
      "phpunit/phpunit": "^9.5"
   },
   "support": {
      "wiki": "https://github.com/AndrykVP/Rancor/wiki"
   },
   "autoload": {
      "psr-4": {
         "AndrykVP\\Rancor\\": "src/",
         "AndrykVP\\Rancor\\DB\\Factories\\": "database/factories",
         "AndrykVP\\Rancor\\DB\\Seeders\\": "database/seeders"
      }
   },
   "autoload-dev": {
      "psr-4": {
         "AndrykVP\\Rancor\\Tests\\": "tests/"
      }
   },
   "prefer-stable": true,
   "extra": {
      "laravel": {
         "providers": [
            "AndrykVP\\Rancor\\Providers\\PackageServiceProvider"
         ]
      }
   }
}
