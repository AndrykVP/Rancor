{
   "name": "andrykvp/rancor",
   "description": "A Laravel Package to make use of common features required by factions or groups of Star Wars Combine",
   "version": "1.4.0",
   "type": "library",
   "keywords": [
      "laravel",
      "star wars",
      "star wars combine",
      "swcombine",
      "swc"
   ],
   "license": "MIT",
   "authors": [
      {
         "name": "Andrés Velázquez",
         "email": "andriclucid@gmail.com",
         "role": "Developer"
      }
   ],
   "require": {
      "php": "^7.0",
      "laravel/framework": "^8.0",
      "mews/purifier": "^3.2"
   },
   "support": {
      "wiki": "https://github.com/AndrykVP/Rancor/wiki"
   },
   "autoload": {
      "psr-4": {
         "AndrykVP\\Rancor\\": "src/",
         "AndrykVP\\Rancor\\Database\\": "database/" 
      }
   },
   "extra": {
      "laravel": {
         "providers": [
            "AndrykVP\\Rancor\\Providers\\PackageServiceProvider"
         ]
      }
   },
   "require-dev": {
      "orchestra/testbench": "^6.0"
   }
}
