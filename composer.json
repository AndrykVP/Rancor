{
   "name": "andrykvp/swc",
   "description": "A Laravel Package to make use of common features required by factions or groups of Star Wars Combine",
   "version": "1.0.0",
   "type": "library",
   "keywords": [
       "laravel",
       "star wars",
       "star wars combine",
       "swcombine",
       "swc"
   ],
   "license": "MIT",
   "authors": [
       {
           "name": "Andrés Velázquez",
           "email": "andriclucid@gmail.com"
       }
   ],
   "require": {
      "laravel/framework": "^7.0",
      "laravel/tinker": "^2.0"
   },
   "autoload": {
        "classmap": [
            "src/database/migrations"
        ],
       "psr-4": {
           "AndrykVP\\SWC\\": "src/"
       }
   },
   "extra": {
       "laravel": {
           "providers": [
               "AndrykVP\\SWC\\Providers\\SWCServiceProvider"
           ],
           "aliases": {
               "API": "AndrykVP\\SWC\\Facades\\APIFacade",
               "IDGen": "AndrykVP\\SWC\\Facades\\IDGenFacade"
           }
       }
   }
}
