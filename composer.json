{
   "name": "andrykvp/rancor",
   "description": "A Laravel Package to make use of common features required by factions or groups of Star Wars Combine",
   "version": "1.3.3",
   "type": "library",
   "keywords": [
      "laravel",
      "star wars",
      "star wars combine",
      "swcombine",
      "swc"
   ],
   "license": "MIT",
   "authors": [
      {
         "name": "Andrés Velázquez",
         "email": "andriclucid@gmail.com",
         "role": "Developer"
      }
   ],
   "require": {
      "php": "^7.0",
      "laravel/framework": "^7.0",
      "doctrine/dbal": "^2.10",
      "mews/purifier": "^3.2"
   },
   "support": {
      "wiki": "https://github.com/AndrykVP/Rancor/wiki"
   },
   "autoload": {
      "classmap": [
         "database/migrations",
         "database/seeds"
      ],
      "psr-4": {
         "AndrykVP\\Rancor\\": "src/"
      }
   },
   "extra": {
      "laravel": {
         "providers": [
            "AndrykVP\\Rancor\\Providers\\FrameworkServiceProvider"
         ]
      }
   }
}
